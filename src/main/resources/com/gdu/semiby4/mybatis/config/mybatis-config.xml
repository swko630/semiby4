<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-config.dtd">

<configuration>

  <!-- settings -->
  <settings>
    <setting name="mapUnderscoreToCamelCase" value="true"/>
  </settings>
  
  <!-- typeAliases -->
  <typeAliases>
    <typeAlias alias="UserDto"          type="com.gdu.semiby4.dto.UserDto"/>
    <typeAlias alias="AttachDto"          type="com.gdu.semiby4.dto.AttachDto"/>
    <typeAlias alias="BoardDto"          type="com.gdu.semiby4.dto.BoardDto"/>
    <typeAlias alias="CommentDto"          type="com.gdu.semiby4.dto.CommentDto"/>
  </typeAliases>
  
</configuration>
